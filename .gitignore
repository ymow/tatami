# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore pid files, but keep the directory.
/tmp/pids/
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore system configuration files.
/config/credentials.yml.enc
/config/credentials/production.key

# Ignore compiled files.
/public/assets
/public/packs
/public/packs-test
/node_modules
/yarn-error.log

# Ignore precompiled assets in development.
/public/assets/*
/public/packs/*

# Ignore other unnecessary files.
.DS_Store
/*.gem
/*.rbc
/.config
/coverage/
/InstalledFiles
/pkg/
/spec/reports/
/spec/examples.txt
/test/tmp/
/test/version_tmp/
/tmp/

# Ignore specific environment config files.
/config/environments/development/local_env.yml
/config/environments/production/local_env.yml
/config/environments/test/local_env.yml

# Ignore IDE/Editor files.
.idea/
.vscode/
*.swp
*~
.project

